{
  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "version": "1.2",
  "manifest_version": 3,
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "watchlist.html",
  "web_accessible_resources": [
    {
      "resources": [
        "img/detailDeleteGray.png",
        "img/detailDeleteGray2.png",
        "img/detailDeleteBlack2.png",
        "img/detailSaveGray.png",
        "img/detailSaveBlack.png",
        "img/simpleDeleteRed.png",
        "img/simpleSaveGreen.png",
        "img/sortAscending.png",
        "img/sortDescending.png"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}